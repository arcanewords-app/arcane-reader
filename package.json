{
  "name": "arcane-reader",
  "version": "0.1.0",
  "description": "Web UI for Arcane novel translator",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/server.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc",
    "start": "node dist/server.js",
    "kill-port": "node scripts/kill-port.js",
    "predev:force": "node scripts/kill-port.js 3000",
    "dev:force": "tsx watch src/server.ts",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "novel",
    "translation",
    "reader",
    "ui"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@preact/signals": "^2.5.1",
    "@supabase/supabase-js": "^2.90.1",
    "arcane-engine": "*",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "epub-gen": "^0.1.0",
    "express": "^4.18.2",
    "lowdb": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.104.0",
    "preact": "^10.28.2"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.55.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vite": "^5.4.21"
  }
}
